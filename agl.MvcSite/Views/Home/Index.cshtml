@model AGL.MvcSite.Models.CatListResponse

@{
    ViewBag.Title = "Index";
}
@helper PrintCatList(List<string> catList, string ownerGenderText)
{
    <h3>@ownerGenderText</h3>
    <ul>
        @foreach (var cat in catList)
        {
            <li>@cat</li>
        }
    </ul>
}

@PrintCatList(Model.MaleOwnerCats, "Male")
@PrintCatList(Model.FemaleOwnerCats, "Female")
